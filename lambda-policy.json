{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": [
          "s3:GetObject",
          "s3:PutObject"
        ],
        "Resource": [
          "arn:aws:s3:::my-ssm-logs-bucket/ssm-logs/*",
          "arn:aws:s3:::my-ssm-logs-bucket/processed-ssm-logs/*"
        ]
      }
    ]
  }